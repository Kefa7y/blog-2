<%= flash[:notice] %>
<%= flash[:alert] %>
<form action="/" method="get">
    <input type="submit" value="Return to index">
</form>

<h1>Users#login</h1><br>

<%= form_tag("/users/api_login", method: "post") do %>
  <%= label_tag("E-mail:") %><br>
  <%= email_field(:user,:email) %><br>
  <%= label_tag("Password:") %><br>
  <%= password_field(:user,:password) %> <br>
  <%= submit_tag("Log in") %>
<% end %>
