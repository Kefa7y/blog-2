<h1>Session#new</h1><br>

<%= form_tag(session_create_path, method: "post") do %>
  <%= label_tag("E-mail:") %><br>
  <%= email_field(:user,:email) %><br>
  <%= label_tag("Password:") %><br>
  <%= password_field(:user,:password) %> <br>
  <%= submit_tag("Log in") %>
<% end %>
<%= link_to "Don't have an account yet? Sign up!",users_new_path%>
